<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:mvc="http://www.springframework.org/schema/mvc"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:aop="http://www.springframework.org/schema/aop" xmlns:tx="http://www.springframework.org/schema/tx"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
		http://www.springframework.org/schema/mvc
		http://www.springframework.org/schema/mvc/spring-mvc-3.2.xsd">

    <bean class="com.controller.AnswerController" p:answerService-ref="answerService"/>
    <bean class="com.controller.MessagesController" p:messagesService-ref="messagesService"/>
    <bean class="com.controller.QuestionBankController" p:questionBankService-ref="questionBankService"/>
    <bean class="com.controller.StudentController" p:studentService-ref="studentService"/>
    <bean class="com.controller.SubjectController" p:subjectService-ref="subjectService"/>
    <bean class="com.controller.TeacherController" p:teacherService-ref="teacherService"/>
    <bean class="com.controller.TestPaperController" p:testPaperService-ref="testPaperService"/>

    <mvc:annotation-driven></mvc:annotation-driven>
    <!--配置视图解析器-->
    <bean class="org.springframework.web.servlet.view.InternalResourceViewResolver"/>

